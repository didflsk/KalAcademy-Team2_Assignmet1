<Page
    x:Class="PhoneBook.DeleteContact"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EmployeeBook"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="285,126,0,0" Text="Name" TextWrapping="Wrap" VerticalAlignment="Top" Height="52" Width="188"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="285,211,0,0" Text="LastName" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <TextBox x:Name="First_Name" HorizontalAlignment="Left" Margin="456,120,0,0" Text="TextBox" VerticalAlignment="Top" Height="56" Width="348"/>
        <Button x:Name="button" Content="Delete" HorizontalAlignment="Left" Margin="305,344,0,0" VerticalAlignment="Top" Height="71" Width="249" Click="button_Click"/>
        <TextBox x:Name="Last_Name" HorizontalAlignment="Left" Margin="456,205,0,0" Text="TextBox" VerticalAlignment="Top" TextChanged="textBox_TextChanged"/>
        <Button x:Name="Back_Button" Content="Back" HorizontalAlignment="Left" Margin="804,344,0,0" VerticalAlignment="Top" Click="Back_Button_Click"/>

    </Grid>
</Page>
